{
  "name": "schedy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:redis": "concurrently \"npm run redis:check\" \"npm run dev\" --names \"redis,next\" --prefix-colors \"yellow,cyan\"",
    "dev:docker": "concurrently \"npm run redis:docker\" \"npm run dev\" --names \"redis,next\" --prefix-colors \"blue,cyan\"",
    "redis:check": "node scripts/start-redis.js",
    "redis:docker": "node scripts/start-redis-docker.js",
    "redis:stop": "docker stop schedy-redis 2>/dev/null || echo 'Redis container not running'",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@bull-board/api": "^6.15.0",
    "@bull-board/express": "^6.15.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "bull-board": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "lucide-react": "^0.474.0",
    "next": "16.0.10",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "react": "19.2.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.0",
    "prisma": "^6.3.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
